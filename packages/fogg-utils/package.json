{
  "name": "fogg-utils",
  "version": "1.0.2",
  "description": "Fogg Utils",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && NODE_ENV=production babel src --out-dir dist --ignore '**/*.test.js'",
    "lint": "eslint --ext .jsx,.js src",
    "lint:fix": "eslint --ext .jsx,.js --fix src",
    "test": "jest --no-cache --maxWorkers=4",
    "test:verbose": "jest --verbose --no-cache --maxWorkers=4",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "prepublish": "npm run lint && npm run build"
  },
  "author": "Carlos Santana",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/foggdev/fogg.git",
    "directory": "packages/fogg-utils"
  },
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "7.4.5",
    "babel-eslint": "10.0.1",
    "babel7-jest": "1.0.1",
    "cross-env": "5.2.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "eslint": "5.16.0",
    "husky": "2.3.0",
    "jest": "24.8.0"
  }
}
