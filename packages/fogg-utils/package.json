{
  "name": "fogg-utils",
  "version": "1.0.0",
  "description": "Fogg Utils",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:dev": "npm run clean && webpack -w -d --config webpack.config.js",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack.config.js",
    "lint": "eslint --ext .jsx,.js src",
    "lint:fix": "eslint --ext .jsx,.js --fix src",
    "test": "jest --no-cache --maxWorkers=4",
    "test:verbose": "jest --verbose --no-cache --maxWorkers=4",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "prepublish": "npm run lint && npm run build"
  },
  "author": "Carlos Santana",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/foggdev/fogg.git",
    "directory": "packages/fogg-ui"
  },
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTestFramework.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss)$": "identity-obj-proxy"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "babel7-jest": "1.0.1",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.13.0",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1"
  }
}
