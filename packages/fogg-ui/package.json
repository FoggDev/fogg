{
  "name": "fogg-ui",
  "version": "2.1.7",
  "description": "Fogg UI",
  "main": "dist/index.js",
  "scripts": {
    "sandbox": "webpack-dev-server --config webpack.sandbox.config.ts",
    "clean": "rm -rf ./dist",
    "build:dev": "npm run clean && webpack -w -d --config webpack.config.ts",
    "build": "npm run clean && NODE_ENV=production webpack --display-modules --config webpack.config.ts",
    "lint": "eslint --ext .tsx,.ts src",
    "lint:fix": "eslint --ext .tsx,.ts --fix src",
    "test": "jest --no-cache --maxWorkers=4",
    "test:verbose": "jest --verbose --no-cache --maxWorkers=4",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "prepublish": "npm run lint && npm run build"
  },
  "author": "Carlos Santana",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/foggdev/fogg.git",
    "directory": "packages/fogg-ui"
  },
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTestFramework.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss)$": "identity-obj-proxy"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/slug": "^0.9.1",
    "@types/styled-components": "^5.0.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.6",
    "babel7-jest": "1.0.1",
    "classnames": "2.2.6",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "moment": "2.24.0",
    "node-sass": "4.13.1",
    "sass-loader": "8.0.2",
    "slug": "^2.1.1",
    "styled-components": "^5.0.1",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11"
  },
  "peerDependencies": {
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "file-loader": "^5.1.0",
    "prettier": "^1.19.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "svg-inline-loader": "^0.8.2",
    "svg-url-loader": "^4.0.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack-dev-server": "^3.10.3"
  }
}
